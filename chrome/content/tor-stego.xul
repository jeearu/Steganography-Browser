    <!-- Firefox -->
    <toolbarpalette id="BrowserToolbarPalette">
        <toolbarbutton id="tor-stego-toolbarbutton"
                       class="toolbarbutton"
                       label="Tor-Stego"
                       tooltiptext="Tor-Stego Extension"
                       oncommand="TorOverlay.CustomButton.click(event);" />

        <!-- More buttons here. -->
    </toolbarpalette>



</overlay>
